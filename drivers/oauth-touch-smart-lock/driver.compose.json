{
  "id": "touch-smart-lock",
  "name": {
    "en": "Touch Smart Lock",
    "nl": "Touch Smart Lock"
  },
  "platforms": [
    "local",
    "cloud"
  ],
  "connectivity": ["cloud"],
  "class": "lock",
  "capabilities": [
    "locked",
    "measure_battery",
    "lock_state"
  ],
  "energy": {
    "batteries": ["AAA", "AAA","AAA", "AAA","AAA", "AAA","AAA", "AAA"]
  },
  "capabilitiesOptions": {
    "lock_state": {
      "preventTag": true
    }
  },
  "images": {
    "large": "/drivers/touch-smart-lock/assets/images/large.jpg",
    "small": "/drivers/touch-smart-lock/assets/images/small.jpg"
  },
  "icon": "/drivers/touch-smart-lock/assets/images/icon.svg",
  "$extends": "loqed"
}
